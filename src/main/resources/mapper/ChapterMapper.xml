<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yihui.huioj.mapper.ChapterMapper">

    <resultMap id="BaseResultMap" type="com.yihui.huioj.model.entity.Chapter">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="bookId" column="bookId" jdbcType="BIGINT"/>
            <result property="title" column="title" jdbcType="VARCHAR"/>
            <result property="orderNum" column="orderNum" jdbcType="INTEGER"/>
            <result property="createTime" column="createTime" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="updateTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,bookId,title,
        orderNum,createTime,updateTime
    </sql>
</mapper>
